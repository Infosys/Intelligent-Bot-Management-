<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ModelObjectRetrievers" type="Infosys.Lif.LegacyWorkbench.Configurations.RetrieversHandler,LegacyParserComponents"/>
    <section name="ContractRetrievers" type="Infosys.Lif.LegacyWorkbench.Configurations.RetrieversHandler,LegacyParserComponents"/>
    <section name="LegacyWorkbenchConfigurations" type="Infosys.Lif.LegacyWorkbench.Configurations.WorkbenchConfigurationsHandler,LegacyWorkBench"/>

  </configSections>
   <connectionStrings>
      <add name="Infosys.Lif.LegacyWorkbench.Properties.Settings.CommerceConnectionString"
         connectionString="Data Source=.;Initial Catalog=Commerce;Integrated Security=True"
         providerName="System.Data.SqlClient" />
   </connectionStrings>
   <LegacyWorkbenchConfigurations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <XsdObjectGeneratorPath>C:\XsdObjGen\XSDObjectGen.exe</XsdObjectGeneratorPath>
    <ModelObjectImporter>
      <assembly>LegacyParserComponents</assembly>
      <fullType>Infosys.Lif.LegacyWorkbench.ModelObjectsImporter</fullType>
    </ModelObjectImporter>
    <ContractsImporter>
      <assembly>LegacyParserComponents</assembly>
      <fullType>Infosys.Lif.LegacyWorkbench.ContractsImporter</fullType>
    </ContractsImporter>
    <CodeGenerator>
      <assembly>LegacyParserComponents</assembly>
      <fullType>Infosys.Lif.LegacyWorkbench.CodeProviders.CobolCodeGenerator</fullType>
    </CodeGenerator>
    <!--CodeGenerator>
      <assembly>LegacyParserComponents</assembly>
      <fullType>Infosys.Lif.LegacyWorkbench.CodeProviders.TINSCodeGenerator</fullType>
    </CodeGenerator-->
     <CodeGeneratedNamespaces>
       <ModelObjectNamespaces>
         <DataEntityNamespace>ModelObjectDataEntity.#EntityName#</DataEntityNamespace>
         <DataEntityRootNamespace>ModelObjectDataEntity</DataEntityRootNamespace>
         <SerializerNamespace>ModelObjectSerializer</SerializerNamespace>
         <SerializerRootNamespace>ModelObjectSerializer</SerializerRootNamespace>
         <HostAccessNamespace>HostAccess.#ModuleName#</HostAccessNamespace>
         <HostAccessRootNamespace>HostAccess.#ModuleName#</HostAccessRootNamespace>
         <XmlNamespace>http://Infosys.ModelObject/CurrentYear/CurrentMonth/#EntityName#</XmlNamespace>
       </ModelObjectNamespaces>
       <ContractNamespaces>
         <DataEntityNamespace>ContractDataEntity.#EntityName#</DataEntityNamespace>
         <DataEntityRootNamespace>ContractDataEntity</DataEntityRootNamespace>
         <SerializerNamespace>ContractSerializer</SerializerNamespace>
         <SerializerRootNamespace>ContractSerializer</SerializerRootNamespace>
         <HostAccessNamespace>HostAccess.#ModuleName#</HostAccessNamespace>
         <HostAccessRootNamespace>HostAccess.#ModuleName#</HostAccessRootNamespace>
         <XmlNamespace>http://Infosys.Contract/CurrentYear/CurrentMonth/#EntityName#</XmlNamespace>
       </ContractNamespaces>
     </CodeGeneratedNamespaces>
     <ReportingConfigurations>
       <Reports>
         <Report Name="CodeGenerationReport" Alias="Code Generation Report" FileLocation="Infosys.Lif.LegacyWorkbench.Reports.CodeGenerationReport.rdlc"></Report>
         <Report Name="EffortSavingReport_CapersJones" Alias="Effort Saving Report-Capers Jones" FileLocation="Infosys.Lif.LegacyWorkbench.Reports.EffortSavingReport.rdlc"></Report>
         <Report Name="EffortSavingReport_Custom" Alias="Effort Saving Report-Custom" FileLocation="Infosys.Lif.LegacyWorkbench.Reports.EffortSavingReport.rdlc"></Report>
         <Report Name="CommentsByLOCRatioReport" Alias="Quality Efficiency Report" FileLocation="Infosys.Lif.LegacyWorkbench.Reports.CommentsByLOCRatioReport.rdlc"></Report>
         <!--<Report Name="SDLCReport"  Alias="SDLC Report"  FileLocation="Infosys.Lif.LegacyWorkbench.Reports.SDLCReport.rdlc"></Report>-->
       </Reports>
       <FileTypeAssociations>
         <!-- File extensions corresponding to the Code Type (file attribute in xml) to be added to this group -->
         <FileType Key="ExtensionsCodeGroup" Value=".csproj;.cs;.svc;.sln;.asmx" ></FileType>
         <!-- File extensions corresponding to the XML Type (file attribute in xml) to be added to this group -->
         <FileType Key="ExtensionsXMLGroup" Value=".config" />
         <!-- File extensions corresponding to the XSD Type (file attribute in xml) to be added to this group -->
         <FileType Key="ExtensionsXSDGroup" Value=".xsd" />
         <!--Any file type for which report statistics(LOC,Comments Count, Number of blank lines) are to be computed, put in this group-->
         <FileType Key="FileStatisticsGroup" Value=".cs;.svc;.config;.xsd;.asmx;.csproj;.sln" />
       </FileTypeAssociations>
       <EffortAnalysis>
         <CapersJones>
           <ExtensionsToAnalyze>
             <ExtensionMapping GroupKey="ExtensionsCodeGroup" ExcludeExtensions=".csproj;.sln" CodeLanguage="C#"></ExtensionMapping>
             <ExtensionMapping GroupKey="ExtensionsXMLGroup" ExcludeExtensions="" CodeLanguage="XML"></ExtensionMapping>
             <ExtensionMapping GroupKey="ExtensionsXSDGroup" ExcludeExtensions="" CodeLanguage="XML"></ExtensionMapping>
           </ExtensionsToAnalyze>
           <FunctionPointMapping>
             <Language Name="C#" UnitLOCPerFunctionPoint="25" PersonHrsPerFunctionPoint="14"></Language>
             <Language Name="XML" UnitLOCPerFunctionPoint="15" PersonHrsPerFunctionPoint="14"></Language>
           </FunctionPointMapping>
         </CapersJones>
         <Custom>
           <ExtensionsToAnalyze>
             <ExtensionMapping GroupKey="ExtensionsCodeGroup" ExcludeExtensions=".csproj;.sln" CodeLanguage="C#"></ExtensionMapping>
             <ExtensionMapping GroupKey="ExtensionsXMLGroup" ExcludeExtensions="" CodeLanguage="XML"></ExtensionMapping>
             <ExtensionMapping GroupKey="ExtensionsXSDGroup" ExcludeExtensions="" CodeLanguage="XML"></ExtensionMapping>
           </ExtensionsToAnalyze>
           <FunctionPointMapping>
             <Language Name="C#" UnitLOCPerFunctionPoint="100" PersonHrsPerFunctionPoint="14"></Language>
             <Language Name="XML" UnitLOCPerFunctionPoint="100" PersonHrsPerFunctionPoint="14"></Language>
           </FunctionPointMapping>
         </Custom>
         <EffortSavedUsingLIF>100</EffortSavedUsingLIF>
       </EffortAnalysis>
     </ReportingConfigurations>
  </LegacyWorkbenchConfigurations >
  <ModelObjectRetrievers defaultRetriever="SmallTalk">
    <Retriever name="COBOL" type="Infosys.Lif.LegacyWorkbench.Cobol" fileExtension="cob" assemblyPath="LegacyParserComponents.dll"/>
    <Retriever name="SmallTalk" type="Infosys.Lif.LegacyWorkbench.SmallTalkConverter" fileExtension="st" assemblyPath="LegacyParserComponents.dll"/>
  </ModelObjectRetrievers>
  <ContractRetrievers defaultRetriever="Goose">
    <Retriever name="Goose" type="Infosys.Lif.LegacyWorkbench.GooseReader" fileExtension="txt" assemblyPath="LegacyParserComponents.dll"/>
    <Retriever name="CSV" type="Infosys.Lif.LegacyWorkbench.CsvReader" fileExtension="csv" assemblyPath="LegacyParserComponents.dll"/>
  </ContractRetrievers>
</configuration>